%!PS

% 2 5 4 3 -> 5

/max
{
    % base case, zero or 1 thing on stack, do nothing

    % recursive case, more than one thing on the stack

    count 1 le
    {
        1 dict begin
        
            /top exch def
            /nextTop exch def

            top nextTop gt
            {
                top
            }
            {
                nextTop
            }
            ifelse

        end

        max
    }
    if
    
}
def