%!
/buf 65535 string def
{ 
   (%stdin) (r) file buf readline not { exit } if
   buf (\0) search {
       /floor 0 def
       /position 0 def
       /first true def
       exch pop exch pop
       length 1 sub 0 exch 1 exch { 
          dup /index exch def
          buf index get dup 40 eq {
              floor 1 add /floor exch def
          } if
          41 eq { 
              floor 1 sub /floor exch def
              floor -1 eq first and {
                  /position index 1 add def
                  /first false def
              } if
          } if
          buf index 0 put
       } for
       floor buf cvs =
       position buf cvs =
   } if
} loop
