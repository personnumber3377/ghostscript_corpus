%!
%%BoundingBox: 110 90 225 264

/makeoutlinedict 7 dict def
/MakeOutlineFont
{ makeoutlinedict begin
  /uniqueid exch def
  /strokewidth exch def
  /newfontname exch def
  /basefontname exch def
  
  /basefontdict basefontname findfont def
  /numentries basefontdict maxlength 1 add def

  basefontdict /UniqueID known not
  { /numentries numentries 1 add def
  } if

  /outfontdict numentries dict def

  basefontdict
  { exch dup /FID ne
    { exch outfontdict 3 1 roll put
    }
    { pop pop
    }
    ifelse
  }
  forall

  outfontdict /FontName newfontname put
  outfontdict /PaintType 2 put
  outfontdict /StrokeWidth strokewidth put
  outfontdict /UniqueID uniqueid put
  newfontname outfontdict definefont pop
  end
} def

/Times-Roman /Times-Outline 1000 1000 div
/Times-Roman findfont dup /UniqueID known
{ /UniqueID get 1 add
}
{ pop 1
}
ifelse
MakeOutlineFont

gsave
100 100 translate
gsave
    gsave
	/Times-Roman findfont 250 scalefont setfont
	0 setgray 6 4 moveto (S) show
	1 setgray 0 0 moveto (S) show
    grestore
    /Times-Outline findfont 250 scalefont setfont
    0 0 moveto (S) show
grestore
gsave 52 0 translate 0.8 1 scale
    gsave
	/Times-Roman findfont 132 scalefont setfont
	0 setgray 2 1.5 moveto (I) show
	1 setgray 0 0 moveto (I) show
    grestore
    0 0 moveto
    /Times-Outline findfont 132 scalefont setfont
    (I) show
grestore
gsave 47 113 translate 0.8 1 scale
    gsave
	/Times-Roman findfont 51 scalefont setfont
	0 setgray 2 1.5 moveto (W) show
	1 setgray 0 0 moveto (W) show
    grestore
    0 0 moveto
    /Times-Outline findfont 51 scalefont setfont
    (W) show
grestore
