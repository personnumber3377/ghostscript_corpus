%!
% UvA Logo in PostScript
% Jan Wielemaker
%%BoundingBox: 30 22 220 410

/uvacross
	{ gsave
	  45 rotate
	  newpath

	  1 1 4
	    { 0.5 0.5 moveto
	      0   2   rlineto
	      -1  0   rlineto
	      0  -2   rlineto
	      90 rotate
	      pop			% pop the control variable
	    }
	  for

	  stroke
	  grestore
	} def

/uvau					% do the U
	{ gsave
	     -3.8  9.2 translate
	     0.55 0.6 scale
	     newpath
	     0 0 moveto
	     0.3  -4.0  0.3  -7    0.1  -9.5 curveto
	     1.3 -11.5  8.0 -12.3 11.7  -9.0 curveto
	    11.5 -12.0 lineto
	    14.0 -11.6 lineto
	    13.6  -8.0 13.6  -4.0 13.8   0.5 curveto
	    13.0   0.3 12.0   0.3 11.5   0.4 curveto
	    11.7  -3.0 11.7  -5.0 11.5  -7.5 curveto
	    10.0  -9.8  5.0 -10.5  3.0  -9.0 curveto
	     2.3  -8.2  2.1  -4.0  2.4   0.0 curveto
	     closepath
	     stroke
	  grestore
	} def

/uva
	{ [0 6.2 11.3]
	    { gsave
	      0 exch translate
	      uvacross
	      grestore
	    }
	  forall

	  uvau
	} def

    gsave
    25 25 scale
    5 3 translate
    0.07 setlinewidth
    uva
    grestore

%showpage
